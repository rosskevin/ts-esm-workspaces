{
  "name": "@rosskevin/ts-esm-workspaces",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "yarn@3.2.1",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc -b packages",
    "rebuild": "run-s clean build",
    "clean": "tsc -b packages --clean && rm -rf packages/*/dist && rm -rf packages/*/test/dist && rm -rf packages/*/benchmark/dist && rm -rf packages/*/coverage",
    "dist": "snowpack build",
    "manipulate-dist-package-main-must-be-removed!!!": "node scripts/build-prod.js"
  },
  "devDependencies": {
    "@tsconfig/create-react-app": "^1.0.2",
    "@types/node": "^17.0.35",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
}
