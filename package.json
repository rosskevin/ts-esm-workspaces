{
  "name": "@rosskevin/ts-esm-workspaces",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "yarn@3.2.1",
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -b",
    "rebuild": "run-s clean build",
    "clean": "tsc -b --clean && rm -rf packages/*/dist"
  },
  "devDependencies": {
    "@tsconfig/create-react-app": "^1.0.2",
    "@types/node": "^18.7.23",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "@rosskevin/ts-esm-workspaces-shared": "workspace:*"
  }
}
